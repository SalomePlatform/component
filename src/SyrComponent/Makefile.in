top_srcdir=@top_srcdir@
top_builddir=../..
srcdir=@srcdir@
VPATH=.:@srcdir@:@top_srcdir@/idl


@COMMENCE@

#EXPORT_SHAREDPYSCRIPTS= COMPONENT_shared_modules.py

EXPORT_HEADERS =  \
	SyrComponent_Impl.hxx 

# Libraries targets
LIB = libSyrComponentEngine.la
LIB_SRC = \
	SyrComponent_Impl.cxx 

LIB_SERVER_IDL = AddComponent.idl \
                 SALOME_Component.idl \
                 SyrComponent.idl

# Executables targets
BIN = SyrComponent_CheckOfUndefined
BIN_SRC = 
BIN_SERVER_IDL =

CPPFLAGS+= $(PYTHON_INCLUDES) $(KERNEL_CXXFLAGS)
LDFLAGS+= $(KERNEL_LDFLAGS) -lSalomeContainer -lSalomeNS -lOpUtil -lRegistry \
          -lSalomeNotification -lSALOMELocalTrace \
          -lSalomeResourcesManager -lSALOMEBasics -lSalomeIDLKernel

LDFLAGSFORBIN+= $(KERNEL_LDFLAGS) -lSalomeContainer -lSalomeNS -lOpUtil -lRegistry \
                -lSalomeNotification -lSALOMELocalTrace \
                -lSalomeResourcesManager -lSALOMEBasics -lSalomeIDLKernel


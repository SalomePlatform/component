top_builddir=../..
srcdir=@srcdir@
VPATH=.:@srcdir@:@top_srcdir@/idl


@COMMENCE@

EXPORT_HEADERS =  \
	DataStreamComponent_Impl.hxx

# Libraries targets
LIB = libDataStreamFactoryEngine.la
LIB_SRC = \
	DataStreamComponent_Impl.cxx 

